{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalDependencies": [
    "package.json",
    "turbo.json",
    "pnpm-workspace.yaml",
    ".env*",
    "scripts/env-setup.ts"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^db:generate", "^build"],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*",
        "scripts/env-setup.js",
        "!**/*.test.{ts,tsx}",
        "!**/*.spec.{ts,tsx}",
        "!**/node_modules/**"
      ],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "env": [
        "NODE_ENV",
        "TURBO_ENV",
        "DATABASE_URL",
        "API_PORT",
        "WEB_PORT",
        "BOT_PORT",
        "API_URL",
        "WEB_URL",
        "LOG_LEVEL",
        "SKIP_DB_INIT",
        "DISCORD_BOT_TOKEN",
        "DISCORD_TOKEN"
      ]
    },
    "dev": {
      "dependsOn": ["^db:generate"],
      "cache": false,
      "persistent": true,
      "env": [
        "NODE_ENV",
        "TURBO_ENV",
        "DATABASE_URL",
        "API_PORT",
        "WEB_PORT",
        "BOT_PORT",
        "API_URL",
        "WEB_URL",
        "NEXTAUTH_URL",
        "LOG_LEVEL",
        "DISCORD_BOT_TOKEN",
        "DISCORD_TOKEN",
        "DISCORD_CLIENT_ID",
        "DISCORD_CLIENT_SECRET",
        "BETTER_AUTH_SECRET",
        "DISCORD_REDIRECT_URI",
        "NEXT_PUBLIC_API_URL",
        "NEXT_PUBLIC_DISCORD_CLIENT_ID",
        "NEXT_PUBLIC_GUILD_ID",
        "REDIS_URL",
        "API_SECRET",
        "API_HOST",
        "LOG_REQUESTS"
      ]
    },
    "start": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true,
      "env": [
        "NODE_ENV",
        "TURBO_ENV",
        "DATABASE_URL",
        "API_PORT",
        "WEB_PORT",
        "BOT_PORT",
        "API_URL",
        "NEXTAUTH_URL",
        "LOG_LEVEL",
        "DISCORD_BOT_TOKEN",
        "DISCORD_TOKEN",
        "LOG_REQUESTS"
      ]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "inputs": ["src/**", "eslint.config.*", ".eslintrc.*"],
      "outputs": [],
      "env": [
        "DEV_PERMISSIONS_HEX",
        "PERMISSION_CACHE_TTL",
        "DEFAULT_ROLES_CACHE_TTL",
        "USER_ROLES_CACHE_TTL",
        "GUILD_OWNER_CACHE_TTL",
        "RUNNING_IN_DOCKER",
        "NEXT_PUBLIC_API_URL",
        "COOKIE_CACHE_MAX_AGE",
        "RATE_LIMIT_ENABLED",
        "RATE_LIMIT_WINDOW",
        "RATE_LIMIT_MAX",
        "COOKIE_DOMAIN",
        "REDIS_URL",
        "API_HOST",
        "DISCORD_TOKEN",
        "DISCORD_BOT_TOKEN",
        "BOT_OWNER_ID",
        "LOG_LEVEL",
        "LOG_REQUESTS",
        "DEV_GUILD_ID",
        "SEED_ENVIRONMENT",
        "SEED_CLEAR_DATA",
        "SEED_ENABLE_LOGGING",
        "NEON_BRANCH_NAME"
      ]
    },
    "type-check": {
      "dependsOn": ["^type-check"],
      "inputs": ["src/**", "tsconfig.json"],
      "outputs": []
    },
    "db:generate": {
      "cache": false,
      "inputs": ["prisma/schema.prisma"],
      "outputs": ["node_modules/.prisma/**"],
      "env": ["DATABASE_URL", "DIRECT_URL"]
    },
    "db:push": {
      "cache": false,
      "env": ["DATABASE_URL", "DIRECT_URL"]
    },
    "db:push:force": {
      "cache": false,
      "env": ["DATABASE_URL", "DIRECT_URL"]
    },
    "db:migrate": {
      "cache": false,
      "env": ["DATABASE_URL", "DIRECT_URL"]
    },
    "db:init": {
      "cache": false,
      "env": ["DATABASE_URL"]
    },
    "db:studio": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "inputs": ["src/**", "tsconfig.json", "tsconfig.tsbuildinfo"],
      "outputs": [],
      "cache": true
    },
    "test": {
      "dependsOn": ["^build"],
      "inputs": ["src/**", "dist/**"],
      "outputs": ["screenshots/**"],
      "cache": false
    },
    "test:e2e": {
      "dependsOn": ["^build"],
      "inputs": ["src/**", "dist/**"],
      "outputs": ["screenshots/**"],
      "cache": false
    },
    "e2e-test": {
      "dependsOn": ["^build"],
      "inputs": ["packages/e2e/**", "apps/web/**"],
      "outputs": ["packages/e2e/screenshots/**"],
      "cache": false
    },
    "test:hurl": {
      "dependsOn": ["^build"],
      "inputs": ["tests/**/*.hurl", "tests/run-tests.sh"],
      "outputs": ["tests/reports/**"],
      "env": ["TEST_USER_TOKEN", "TEST_GUILD_ID", "API_URL"],
      "cache": false
    }
  }
}
