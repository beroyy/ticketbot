{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalDependencies": [
    "package.json",
    "turbo.json",
    "pnpm-workspace.yaml",
    ".env*"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^db:generate", "^build"],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*",
        "!**/*.test.{ts,tsx}",
        "!**/*.spec.{ts,tsx}",
        "!**/node_modules/**"
      ],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "env": [
        "NODE_ENV",
        "DATABASE_URL",
        "DIRECT_URL",
        "BETTER_AUTH_SECRET",
        "NEXT_PUBLIC_*",
        "TURBO_ENV",
        "NEON_BRANCH_NAME"
      ]
    },
    "dev": {
      "dependsOn": ["^db:generate"],
      "cache": false,
      "persistent": true,
      "env": [
        "NODE_ENV",
        "DATABASE_URL",
        "REDIS_URL",
        "BETTER_AUTH_SECRET",
        "DISCORD_*",
        "NEXT_PUBLIC_*",
        "API_*",
        "WEB_*",
        "BOT_*",
        "DEV_*",
        "COOKIE_*",
        "RATE_LIMIT_*",
        "LOG_*",
        "*_CACHE_TTL",
        "TURBO_ENV",
        "NEON_BRANCH_NAME"
      ]
    },
    "start": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true,
      "env": [
        "NODE_ENV",
        "DATABASE_URL",
        "REDIS_URL",
        "BETTER_AUTH_SECRET",
        "DISCORD_*",
        "NEXT_PUBLIC_*",
        "API_*",
        "WEB_*",
        "BOT_*",
        "COOKIE_*",
        "RATE_LIMIT_*",
        "LOG_*",
        "*_CACHE_TTL"
      ]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "inputs": ["src/**", "eslint.config.*", ".eslintrc.*"],
      "outputs": []
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "inputs": ["src/**", "tsconfig.json", "tsconfig.tsbuildinfo"],
      "outputs": [],
      "cache": true
    },
    "test": {
      "dependsOn": ["^build"],
      "inputs": ["src/**", "tests/**", "vitest.config.*"],
      "outputs": ["coverage/**"],
      "cache": false,
      "env": [
        "NODE_ENV",
        "DATABASE_URL"
      ]
    },
    "test:hurl": {
      "dependsOn": ["^build"],
      "inputs": ["tests/**/*.hurl", "tests/run-tests.sh"],
      "outputs": ["tests/reports/**"],
      "env": ["TEST_USER_TOKEN", "TEST_GUILD_ID", "API_URL"],
      "cache": false
    },
    "db:generate": {
      "cache": false,
      "inputs": ["prisma/schema.prisma"],
      "outputs": ["node_modules/.prisma/**"],
      "env": ["DATABASE_URL", "DIRECT_URL"]
    },
    "db:push": {
      "cache": false,
      "env": ["DATABASE_URL", "DIRECT_URL"]
    },
    "db:push:force": {
      "cache": false,
      "env": ["DATABASE_URL", "DIRECT_URL"]
    },
    "db:migrate": {
      "cache": false,
      "env": ["DATABASE_URL", "DIRECT_URL"]
    },
    "db:studio": {
      "cache": false,
      "persistent": true,
      "env": ["DATABASE_URL"]
    },
    "clean": {
      "cache": false
    }
  }
}