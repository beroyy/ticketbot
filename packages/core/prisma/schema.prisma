generator client {
  provider = "prisma-client-js"
  previewFeatures = ["driverAdapters", "postgresqlExtensions"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum TicketStatus {
  OPEN
  CLAIMED
  CLOSED
  PENDING
}

enum PanelType {
  SINGLE
  MULTI
}

enum FormFieldType {
  SHORT_TEXT
  PARAGRAPH
  SELECT
  EMAIL
  NUMBER
  CHECKBOX
  RADIO
  DATE
}

enum GuildRoleStatus {
  ACTIVE
  INACTIVE
}

enum EventCategory {
  TICKET
  TEAM
  PANEL
  MEMBER
  GUILD
}

enum EventTargetType {
  TICKET
  ROLE
  USER
  PANEL
  GUILD
}