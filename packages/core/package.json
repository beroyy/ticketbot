{
  "name": "@ticketsbot/core",
  "version": "0.0.1",
  "description": "Core schemas, validation, utilities, and database layer for TicketsBot",
  "type": "module",
  "scripts": {
    "dev": "tsc --watch --noEmit",
    "build": "tsc --project tsconfig.json --noEmit",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint --config eslint.config.js src/"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "@sapphire/bitfield": "^1.2.4",
    "better-auth": "^1.3.2",
    "better-call": "^1.0.12",
    "bullmq": "^5.56.5",
    "discord.js": "^14.21.0",
    "hono": "^4.8.5",
    "redis": "^5.6.0",
    "ws": "^8.18.3",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@ticketsbot/db": "workspace:*",
    "@ticketsbot/eslint-config": "workspace:*",
    "@ticketsbot/tsconfig": "workspace:*",
    "@types/ws": "^8.18.1",
    "bufferutil": "^4.0.9",
    "dotenv": "^17.2.0",
    "eslint": "^9.31.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts",
    "./prisma": {
      "import": "@ticketsbot/db",
      "require": "@ticketsbot/db"
    },
    "./domains": "./src/domains/index.ts",
    "./domains/*": "./src/domains/*/index.ts",
    "./domains/*/client": "./src/domains/*/client.ts",
    "./discord": "./src/discord/index.ts",
    "./context": "./src/context/index.ts",
    "./auth": "./src/auth/index.ts",
    "./auth/client": "./src/auth/client.ts",
    "./analytics": "./src/analytics/index.ts"
  },
  "prisma": {
    "schema": "./prisma/"
  }
}
