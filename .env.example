# === Core Settings ===
NODE_ENV="development"
TURBO_ENV="dev"
WORKTREE_ID="your_worktree_id"
GIT_BRANCH_NAME="your_branch_name"

# === Database ===
DATABASE_URL="postgresql://username:password@localhost:5432/ticketsbot_dev"
DIRECT_URL="postgresql://username:password@localhost:5432/ticketsbot_dev"
NEON_BRANCH_NAME="your_branch_name"
NEON_PROJECT_ID="your_project_id"
NEON_API_KEY="your_neon_api_key"
NEON_PARENT_BRANCH="main"
MAIN_BRANCH_DATABASE_URL="postgresql://username:password@localhost:5432/ticketsbot_main"

# === Services ===
PORT_LEVEL="4"
AUTO_PORT_DETECTION="true"
WEB_PORT="4000"
API_PORT="4001"
BOT_PORT="4002"
WEB_URL="http://localhost:4000"
API_URL="http://localhost:4001"
NEXT_PUBLIC_API_URL="http://localhost:4001"
API_HOST="0.0.0.0"
API_SECRET="your_api_secret_key"
REDIS_URL="redis://localhost:4379"
REDIS_PORT="6379"

# === Authentication ===
DISCORD_CLIENT_ID="your_discord_client_id"
DISCORD_CLIENT_SECRET="your_discord_client_secret"
DISCORD_TOKEN="your_discord_bot_token"
DISCORD_REDIRECT_URI="http://localhost:4001/auth/callback/discord"
NEXT_PUBLIC_GUILD_ID="your_guild_id"
BETTER_AUTH_SECRET="your_better_auth_secret_at_least_32_chars"

# === Development ===
LOG_LEVEL="info"
DEV_GUILD_ID="your_guild_id"
DEV_ADMIN_PERMS="false"
DEV_DB_AUTO_SEED="true"

