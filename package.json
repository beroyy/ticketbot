{
  "name": "ticketsbot-ai",
  "version": "0.0.1",
  "description": "Discord ticket bot with web dashboard",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "dotenvx run -- turbo build",
    "clean": "rm -rf .turbo node_modules/.cache apps/*/.next apps/*/.turbo packages/*/.turbo",
    "db:generate": "dotenvx run -- pnpm --filter @ticketsbot/db db:generate",
    "db:migrate": "dotenvx run -- pnpm --filter @ticketsbot/db db:migrate",
    "db:push": "dotenvx run -- pnpm --filter @ticketsbot/db db:push",
    "db:studio": "dotenvx run -- pnpm --filter @ticketsbot/db db:studio",
    "db:reset": "dotenvx run -- pnpm --filter @ticketsbot/db db:reset",
    "dev": "dotenvx run -- turbo dev",
    "docker": "docker compose up --build",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "lint": "turbo run lint -- --fix",
    "start": "turbo start",
    "typecheck": "turbo run typecheck"
  },
  "dependencies": {
    "zod": "^4.0.5"
  },
  "overrides": {
    "zod": "^4.0.5",
    "@sapphire/framework": "^5.3.6",
    "discord.js": "^14.21.0",
    "prisma": "^6.12.0",
    "@prisma/client": "^6.12.0",
    "@prisma/adapter-neon": "^6.12.0"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.48.4",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.0.14",
    "concurrently": "^9.2.0",
    "eslint": "^9.31.0",
    "neonctl": "^2.12.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tsx": "^4.20.3",
    "turbo": "^2.5.5",
    "typescript": "^5.8.3"
  }
}
