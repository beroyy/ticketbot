{
  "name": "ticketsbot-ai",
  "version": "0.0.1",
  "description": "Discord ticket bot with web dashboard",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "tsx scripts/env-setup.ts staging && turbo run build",
    "build:api": "turbo run build --filter=@ticketsbot/api",
    "build:bot": "turbo run build --filter=@ticketsbot/bot",
    "build:web": "turbo run build --filter=@ticketsbot/web",
    "clean": "turbo run clean && rm -rf node_modules/.cache/turbo",
    "db": "turbo run db:studio",
    "db:generate": "dotenvx run -- turbo run db:generate",
    "db:migrate": "dotenvx run -- turbo run db:migrate",
    "db:push": "dotenvx run -- turbo run db:push",
    "db:push:force": "dotenvx run -- turbo run db:push:force",
    "db:seed": "dotenvx run -- tsx packages/scripts/src/db/seed.ts",
    "dev": "tsx packages/scripts/src/dev/start-dev-services.ts",
    "dev:no-redis": "tsx scripts/env-setup.ts dev && turbo run dev",
    "dev:api": "turbo run dev --filter=@ticketsbot/api",
    "dev:bot": "turbo run dev --filter=@ticketsbot/bot",
    "dev:docker": "turbo run dev",
    "dev:web": "turbo run dev --filter=@ticketsbot/web",
    "redis:start": "tsx packages/scripts/src/dev/start-redis.ts",
    "redis:stop": "docker stop ticketsbot-redis-$(grep WORKTREE_ID .env | cut -d= -f2 | tr -d '\"') || true",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "setup-env": "tsx packages/scripts/src/dev/env-setup.ts",
    "validate-env": "tsx packages/scripts/src/dev/validate-env.mjs",
    "env:setup": "tsx packages/scripts/src/dev/env-setup.ts",
    "env:validate": "node packages/scripts/src/dev/validate-env.mjs",
    "start": "turbo run start",
    "docker": "docker compose up -d",
    "docker:build": "docker compose build",
    "docker:dev": "tsx packages/scripts/src/dev/env-setup.ts dev && dotenvx run -- docker compose up --build",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:up": "docker compose up -d",
    "test": "turbo run test",
    "test:api": "turbo run test --filter=@ticketsbot/api",
    "test:api:hurl": "turbo run test:hurl --filter=@ticketsbot/api",
    "test:discord": "turbo run test --filter=@ticketsbot/discord",
    "test:e2e": "turbo run test:e2e",
    "test:unit": "turbo run test",
    "test:token": "dotenvx run -- tsx packages/scripts/src/test/generate-test-token.ts",
    "typecheck": "turbo run typecheck",
    "typecheck:api": "turbo run typecheck --filter=@ticketsbot/api",
    "typecheck:bot": "turbo run typecheck --filter=@ticketsbot/bot",
    "typecheck:discord": "turbo run typecheck --filter=@ticketsbot/discord",
    "typecheck:web": "turbo run typecheck --filter=@ticketsbot/web"
  },
  "dependencies": {
    "zod": "^4.0.5"
  },
  "overrides": {
    "zod": "^4.0.5",
    "@sapphire/framework": "^5.3.6",
    "discord.js": "^14.21.0",
    "prisma": "^6.12.0",
    "@prisma/client": "^6.12.0",
    "@prisma/adapter-neon": "^6.12.0"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.0.0",
    "@orangeopensource/hurl": "^6.1.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.0.14",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.31.0",
    "neonctl": "^2.12.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "turbo": "^2.5.5",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  }
}
