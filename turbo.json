{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalDependencies": ["package.json", "turbo.json", "pnpm-workspace.yaml", ".env*"],
  "tasks": {
    "build": {
      "dependsOn": ["^build", "@ticketsbot/core#db:generate"],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*",
        "!**/*.test.{ts,tsx}",
        "!**/*.spec.{ts,tsx}",
        "!**/node_modules/**"
      ],
      "outputs": [".next/**", "!.next/cache/**"],
      "env": [
        "NODE_ENV",
        "DATABASE_URL",
        "BETTER_AUTH_SECRET",
        "DISCORD_CLIENT_ID",
        "DISCORD_CLIENT_SECRET",
        "DISCORD_TOKEN",
        "WEB_URL",
        "API_URL",
        "REDIS_URL",
        "PORT_OFFSET",
        "BASE_DOMAIN",
        "NEXT_PUBLIC_*"
      ]
    },
    "dev": {
      "dependsOn": ["@ticketsbot/core#db:generate"],
      "cache": false,
      "persistent": true,
      "env": [
        "NODE_ENV",
        "DATABASE_URL",
        "REDIS_URL",
        "BETTER_AUTH_SECRET",
        "DISCORD_*",
        "NEXT_PUBLIC_*",
        "API_URL",
        "WEB_URL",
        "PORT_OFFSET",
        "BASE_DOMAIN",
        "DEV_*",
        "LOG_*"
      ]
    },
    "start": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true,
      "env": [
        "NODE_ENV",
        "DATABASE_URL",
        "REDIS_URL",
        "BETTER_AUTH_SECRET",
        "DISCORD_*",
        "NEXT_PUBLIC_*",
        "API_URL",
        "WEB_URL",
        "PORT_OFFSET",
        "BASE_DOMAIN",
        "LOG_*"
      ]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "inputs": ["src/**", "eslint.config.*", ".eslintrc.*"],
      "outputs": [],
      "env": []
    },
    "typecheck": {
      "dependsOn": ["^build", "@ticketsbot/core#db:generate"],
      "inputs": ["src/**", "tsconfig.json", "tsconfig.tsbuildinfo"],
      "outputs": [],
      "cache": true
    },
    "test": {
      "dependsOn": ["^build"],
      "inputs": ["src/**", "tests/**", "vitest.config.*"],
      "outputs": ["coverage/**"],
      "cache": false,
      "env": ["NODE_ENV", "DATABASE_URL"]
    },
    "db:generate": {
      "cache": true,
      "inputs": ["prisma/**/*.prisma"],
      "outputs": ["node_modules/.prisma/**"],
      "env": ["DATABASE_URL"]
    },
    "db:push": {
      "cache": false,
      "env": ["DATABASE_URL"]
    },
    "db:push:force": {
      "cache": false,
      "env": ["DATABASE_URL"]
    },
    "db:migrate": {
      "cache": false,
      "env": ["DATABASE_URL"]
    },
    "db:studio": {
      "cache": false,
      "persistent": true,
      "env": ["DATABASE_URL"]
    },
    "clean": {
      "cache": false
    }
  }
}
